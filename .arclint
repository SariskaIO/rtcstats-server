{
  "linters": {
    "filename": {
      "type": "filename"
    },
    "chmod": {
      "type": "chmod"
    },
    "merge-conflict": {
      "type": "merge-conflict"
    },
    "javascript": {
      "type": "eslint",
      "include": [
        "(^.*\\.js$)"
      ],
      "bin": "./node_modules/.bin/eslint"
    }
  }
}
